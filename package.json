{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "test:ci": "jest",
    "test:snaps": "jest -u --coverage=false",
    "update:tailwind-rn": "npx create-tailwind-rn",
    "prebuild": "yarn update:tailwind-rn",
    "lint": "eslint --color ./src",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue|ts|tsx)\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "markdown-to-jsx": "^7.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.15.3",
    "react-is": "^17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "styled-components": "^5.2.1",
    "sweetalert2": "^10.15.5",
    "tailwind-rn": "^3.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@trivago/prettier-plugin-sort-imports": "^1.4.4",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^9",
    "babel-eslint": "^10.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-order-autofix": "^0.8.3",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-styled-components-a11y": "^0.0.32",
    "eslint-plugin-tailwind": "^0.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.7.9",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-react-redux": "^3.3.2",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "4.3.8",
    "jest-expo": "^40.0.2",
    "lint-staged": "^10.5.4",
    "miragejs": "^0.1.41",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "~4.0.0"
  },
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": "eslint --fix",
    "*.+(json|css|md)": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "jest-expo/universal",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  }
}
